          android:layout_height="wrap_content"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|left"
          android:layout_weight="3"
          android:text="@string/STR_MM_10_04_02_09" android:layout_marginLeft="14dip" style="@style/Style.Text.Text.ListItemText"/>
      <TextView 
          android:id="@+id/my_report_sharp_turn"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|right"
           android:layout_marginRight="14dip" style="@style/Style.Text.Text.ListItemSubText"/>
  </LinearLayout>
  
      <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="66dip"
      android:orientation="horizontal"
      android:background="@drawable/navicloud_and_652a">
      <TextView 
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|left"
          android:layout_weight="3"
          android:text="@string/STR_MM_10_04_02_39" android:layout_marginLeft="14dip" style="@style/Style.Text.Text.ListItemText"/>
      <TextView 
          android:id="@+id/my_report_press_line"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|right"
           android:layout_marginRight="14dip" style="@style/Style.Text.Text.ListItemSubText"/>
  </LinearLayout>
  
         <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="66dip"
      android:orientation="horizontal"
      android:background="@drawable/navicloud_and_652a">
      <TextView 
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|left"
          android:layout_weight="3"
          android:text="@string/STR_MM_10_04_02_40" android:layout_marginLeft="14dip" style="@style/Style.Text.Text.ListItemText"/>
      <TextView 
          android:id="@+id/my_report_car_distance"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|right"
           android:layout_marginRight="14dip" style="@style/Style.Text.Text.ListItemSubText"/>
  </LinearLayout>
  
    <ImageView
      android:layout_width="match_parent"
      android:layout_height="10dip" android:background="@drawable/navicloud_and_721a"/>
    <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="66dip"
      android:orientation="horizontal"
      android:background="@drawable/navicloud_and_652a">
      <TextView 
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|left"
          android:layout_weight="3"
          android:text="@string/STR_MM_10_04_02_32" android:layout_marginLeft="14dip" style="@style/Style.Text.Text.ListItemText"/>
    
      <RatingBar 
          android:id="@+id/my_report_level"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          style="@style/levelRatingBar"
          android:layout_gravity="center_vertical"
          android:gravity="center_vertical|left"
          android:layout_marginRight="14dip"
          android:clickable="false"
          android:numStars="5" android:rating="3"/>
  </LinearLayout>
   <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="66dip"
      android:orientation="horizontal"
      android:background="@drawable/navicloud_and_652a">
      <TextView 
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|left"
          android:layout_weight="3"
          android:text="@string/STR_MM_10_04_02_35" android:layout_marginLeft="14dip" style="@style/Style.Text.Text.ListItemText"/>
      <TextView 
          android:id="@+id/my_report_currently_ranking"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|right"
    	android:layout_marginRight="14dip" style="@style/Style.Text.Text.ListItemSubText"/>
  </LinearLayout>
 </LinearLayout>



</ScrollView>

</RelativeLayout>