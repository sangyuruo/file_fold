          android:text="@string/STR_MM_10_04_02_29" android:layout_marginLeft="14dip" style="@style/Style.Text.Text.ListItemText"/>
      <TextView 
          android:id="@+id/my_report_urgent_slowdown"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_weight="1"
          android:layout_gravity="center_vertical"
    	android:gravity="center_vertical|right"
    	android:layout_marginRight="14dip" style="@style/Style.Text.Text.ListItemSubText"/>
  </LinearLayout>
   <LinearLayout 
      android:layout_width="match_parent"
      android:layout_height="66dip"
      android:orientation="horizontal"
      android:background="@drawable/navicloud_and_652a">
      <TextView 
