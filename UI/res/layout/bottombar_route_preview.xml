<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	>
	<RelativeLayout android:layout_width="wrap_content"
	android:layout_centerHorizontal="true"
	android:id="@+id/bar_two" android:layout_alignParentBottom="true" android:layout_height="61dip">

	<RelativeLayout
		android:id="@+id/bottom_route_layout"
		android:layout_height="61dip" 
		android:layout_centerHorizontal="true"
		android:layout_alignParentBottom="true"
		android:orientation="horizontal"
		android:layout_width="360dip"
		android:paddingLeft="3dip" android:paddingRight="3dip">
	<ImageView   
		android:id="@+id/bottom_route_background"
	    android:background="@drawable/navicloud_and_710a"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="10dip"
	    android:layout_marginLeft="-3dip"
	    android:layout_marginRight="-3dip"
	    android:layout_width="match_parent"
	    android:layout_alignParentBottom="true"
	  	android:layout_height="51dip"/>
		<RelativeLayout android:id="@+id/listdisplay_btn"
			android:layout_height="wrap_content" android:background="@xml/btn_bottombar_1"
			android:layout_width="90dip"
			>
			
			<TextView 
				android:layout_alignParentBottom="true"
				android:text="@string/STR_MM_02_02_03_01"
				android:gravity="center"
				android:layout_width="match_parent" android:layout_height="20dip"
				android:textSize="@dimen/text_size_04"
				android:textColor="@color/navicloud_text_002" />
		</RelativeLayout>
		<RelativeLayout android:id="@+id/roadedit_btn"
			android:layout_height="wrap_content" android:background="@xml/btn_bottombar_2"
			android:layout_width="90dip"
			android:layout_toRightOf="@id/listdisplay_btn"
			android:layout_marginLeft="-3dip"
			>
			<TextView 
				android:layout_alignParentBottom="true"
				android:text="@string/STR_MM_01_02_01_13"
				android:gravity="center"
				android:layout_width="match_parent" android:layout_height="20dip"
				android:textSize="@dimen/text_size_04"
				android:textColor="@color/navicloud_text_002" />
		</RelativeLayout>
		<RelativeLayout android:id="@+id/avoid_btn"
			android:layout_height="wrap_content" android:background="@xml/btn_bottombar_3"
			android:layout_width="90dip"
			android:layout_marginRight="-3dip"
			android:layout_toRightOf="@id/roadedit_btn"
			>
			<TextView 
				android:layout_alignParentBottom="true"
				android:text="@string/STR_COM_059"
				android:gravity="center"
				android:layout_width="match_parent" android:layout_height="20dip"
				android:textSize="@dimen/text_size_04"
				android:textColor="@color/navicloud_text_002" />
		</RelativeLayout>
		<RelativeLayout android:id="@+id/startnavigation_btn"
			android:layout_height="wrap_content" android:background="@xml/btn_bottombar_4"
			android:layout_toRightOf="@id/avoid_btn"
			android:layout_width="90dip"
			>
			<TextView 
				android:layout_alignParentBottom="true"
				android:text="@string/STR_MM_03_02_01_07"
				android:gravity="center"
				android:layout_width="match_parent" android:layout_height="20dip"
				android:textSize="@dimen/text_size_04"
				android:textColor="@color/navicloud_text_002" />
		</RelativeLayout>
	</RelativeLayout>
	</RelativeLayout>
	<RelativeLayout 
		android:layout_above="@id/bar_two"
		android:layout_width="match_parent"
		android:layout_height="wrap_content" android:id="@+id/bar_one"
		>
		<ImageView android:id="@+id/btnleft" android:src="@drawable/bottombar_preview_btn"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			/>
		<ImageView android:id="@+id/btnright" android:src="@drawable/bottombar_next_btn"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:layout_alignParentRight="true" />
	</RelativeLayout>
	<RelativeLayout android:layout_width="wrap_content" android:layout_height="match_parent"
	android:layout_alignParentLeft="true"
	android:layout_centerVertical="true">
		<ImageView android:id="@+id/simulatenavigation_btn"
		android:layout_marginTop="53dip"
		android:layout_centerVertical="true"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:background="@xml/btn_simulate_navigation_selector"/>
	</RelativeLayout>
</RelativeLayout>