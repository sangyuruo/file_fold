<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="268dip"
	android:layout_height="match_parent"
	android:background="@drawable/navicloud_and_732a">
	 <TextView
       android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="26dp"
        android:background="@drawable/navicloud_and_734a"
        android:singleLine="true" android:gravity="center_vertical" android:textSize="14dip" android:textColor="#ffe4e4e4" android:paddingLeft="15dip"/>
	<TextView android:layout_width="match_parent"
			  android:layout_height="wrap_content"
			  android:text="TextView"
		android:id="@+id/textdata"
		 android:paddingBottom="23dp"
                android:paddingLeft="12dp"
                android:paddingRight="12dp"
                android:paddingTop="23dp"
                 android:textSize="14dip" android:layout_gravity="center_horizontal"
		android:textColor="#ff000000"
		/>
<LinearLayout
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:orientation="horizontal"
	android:background="@drawable/navicloud_and_721a"
	android:padding="4dip"
>
	<Button
	android:id="@+id/ok"
	style="@style/Widget.Button"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:layout_weight="1"
	android:text="@string/STR_MM_02_04_02_02"
	android:textSize="14dip"
	android:layout_marginLeft="4dp" android:layout_marginRight="4dp"
	/>
	
	<Button
	android:id="@+id/cancel"
	style="@style/Widget.Button"
	android:layout_height="wrap_content"
	android:layout_width="match_parent"
	android:layout_weight="1"
	android:text="@string/STR_MM_05_02_01_19"
	android:textSize="14dip"
	android:layout_marginLeft="4dp" android:layout_marginRight="4dp"
	/>
</LinearLayout>
</LinearLayout>
