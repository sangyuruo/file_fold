<?xml version="1.0" encoding="utf-8"?>
<com.billionav.navi.component.basiccomponent.RelativeLayoutNoPress
xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/list_Item_Schedule_Dist_RelativeLayout"
    android:layout_width="match_parent"
    android:layout_height="80dip"
    android:paddingLeft="5dip">

	<LinearLayout android:id="@+id/list_Item_Guide_Dist_Linearlayour01"
		android:layout_alignParentTop="true" android:orientation="vertical"
		android:layout_marginLeft="5dip" android:layout_centerVertical="true"
		android:paddingTop="5dip" android:paddingBottom="6dip"
		android:layout_toLeftOf="@+id/list_Item_Schedule_Dist_image"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
		android:layout_marginRight="12dip">

		<TextView android:id="@+id/list_Item_Schedule_Point_name"
				android:layout_height="wrap_content" style="@style/Style.Text.Text.ListItemText"
				android:text="TextView01" android:layout_width="fill_parent"
				android:singleLine="true" />
		<TextView android:id="@+id/list_Item_Schedule_expected_time" style="@style/Style.Text.Text.ListItemSubText"
				android:layout_width="fill_parent" android:layout_height="wrap_content"
				android:singleLine="true" android:text="TextView02" />
		<TextView android:id="@+id/list_Item_Schedule_Point_time"
			android:layout_height="wrap_content" android:layout_centerVertical="true"
			style="@style/Style.Text.Text.ListItemSubText" android:text="TextView03"
			android:layout_width="fill_parent" android:singleLine="true" />
	</LinearLayout>

    <ImageView
        android:id="@+id/list_Item_Schedule_Dist_image"
        android:layout_width="42dip"
        android:layout_alignParentRight="true"
        android:layout_marginRight="12dip" android:layout_centerVertical="true" android:layout_height="42dip"
        android:background="@drawable/list_faverate_route_btn_selector"
        android:scaleType="center"
       />
		
</com.billionav.navi.component.basiccomponent.RelativeLayoutNoPress>